/* pages/finished/finished.wxss */
page{
  background-color: 	#F5F5F5;
}

.container{
  margin: 0px 20rpx 0px 20rpx;
  padding: 10px  0;
  display: flex;
  flex-direction: column;
  background-color: white;
  overflow: hidden;
}

.section{
  width: 100%;
  display: flex;
  flex-direction: row;
}

.icon{
  margin-left: 10rpx;
  margin-right: 40rpx;
  height: 30px;
  width: 30px;
}

.section-title{
  width: 220rpx;
  margin-right: 40rpx;
  height: 30px;
  line-height: 30px;
}

text{
  height: 30px;
  line-height: 30px;
}

.orderType{
  height: 10px;
  line-height: 10px;
  margin-left: 20rpx;
  color: #CDC9C9;
}

button{
  height: 30px;
  line-height: 30px;
  background-color: 	#1E90FF;
  color: white;
}

.status{
  color: #FF0000;
}

.detail{
  margin-right: 10rpx;
  width: 150rpx;
}
